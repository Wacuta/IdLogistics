<!doctype html>  


<html lang="fr">  

    <head>
      <meta charset="utf-8">
      <title>GuideOffice</title>

      <!-- Bootstrap Core CSS -->
      <link href="css/bootstrap.min.css" rel="stylesheet">
      
      <link href="css/style.css" rel="stylesheet" >
      
    </head>



   {% block menu_nav %} 
    
   {% endblock %}
        
     

    <body data-spy="scroll" data-target="#myScrollspy">
        
        {% block header %}
                
          <div class="container">
            <div class="row">
              <div class="col-md-3">
                <img src="images/IDlogistics_Logo.jpg" id="logo">
              </div>
              <div class="col-md-9">
                <h1 id="titreSite">Guide</h1>
                <h1><small>Guide aux applications du pack Office et Oracle</small></h1>
              </div>
            </div>
          </div>


          <nav class="navbar navbar-default ">
             <div class="container">
                <div class="navbar-header">
                  <button type="button" class="navbar-toggle oracle" data-toggle="collapse" >
                    <div><img class="itemLogo" src="images/Logo_Oracle.png"></div>
                  </button>
                  <button type="button" class="navbar-toggle powerpoint" data-toggle="collapse" >
                    <div><img class="itemLogo" src="images/Logo_PowerPoint.png"></div>
                  </button>
                  <button type="button" class="navbar-toggle excel" data-toggle="collapse" >
                    <div><img class="itemLogo" src="images/Logo_Excel.png"></div>
                  </button>
                  <button type="button" class="navbar-toggle word" data-toggle="collapse" >
                    <div><img class="itemLogo" src="images/Logo_Word.png"></div>
                  </button>
                  <button type="button" class="navbar-toggle home" data-toggle="collapse" >
                    <div><img class="itemLogo" src="images/Logo_Home.png"></div>
                  </button>
                </div>
                <div class="collapse navbar-collapse" id="myNavbar">
                   <ul class="nav nav-justified">
                      <li><a href="#" id="ItemHome">
                        <div><img class="itemLogo" src="images/Logo_Home.png"></div> Home
                      </a></li>
                      <li><a href="#sectionWord" id="ItemWord">
                        <div><img class="itemLogo" src="images/Logo_Word.png"></div> Word
                      </a></li>
                      <li><a href="#sectionExcel" id="ItemExcel">
                        <div><img class="itemLogo" src="images/Logo_Excel.png"></div> Excel
                      </a></li>
                      <li><a href="#sectionPowerPoint" id="ItemPowerPoint">
                        <div><img class="itemLogo" src="images/Logo_PowerPoint.png"></div> PowerPoint
                      </a></li>
                      <li><a href="#sectionOracle" id="ItemOracle">
                        <div><img class="itemLogo" src="images/Logo_Oracle.png"></div> SQL
                      </a></li>
                   </ul>
                  </div>
             </div>
          </nav>
            
            
        {% endblock %}

    


       {% block contenue %}

       <div class="row" id="nav">
        <div class="col-xs-3" id="myScrollspy">
            <ul class="nav list-group affix-top" data-spy="affix" data-offset-top="300">
                <li class="list-group-item"><a href="#" class="">Accueil</a></li>
                <li class="list-group-item"><a href="#prepa" class="">Pr√©sentation</a></li>
                <li class="list-group-item"><a href="#cours" class="">Cours</a>
                    <ul class="nav submenu">
                        <li class="list-group-item hide"><a href="#coursWord" class="">Word</a></li>
                        <li class="list-group-item hide"><a href="#coursExcel" class="">Excel</a></li>
                        <li class="list-group-item hide"><a href="#coursPowerPoint" class="">PowerPoint</a></li>
                        <li class="list-group-item hide"><a href="#coursOracle" class="">Oracle</a></li>
                    </ul>
                </li>
                <li class="list-group-item"><a href="#exercice" class="">Exercice</a>
                    <ul class="nav submenu">
                        <li class="list-group-item hide"><a href="#exerciceWord" class="">Word</a></li>
                        <li class="list-group-item hide"><a href="#exerciceExcel" class="">Excel</a></li>
                        <li class="list-group-item hide"><a href="#exercicePowerPoint" class="">PowerPoint</a></li>
                        <li class="list-group-item hide"><a href="#exerciceOracle" class="">Oracle</a></li>
                    </ul>
                </li>
                <li class="list-group-item"><a href="#contact" class="">Contact</a></li>
            </ul>
        </div>
        

        <!-- PARTIE COURS -->
        <div class="col-xs-6">
          <div class="part" id="cours">
            <h1 class="part-heading text-center"> Cours</h1>
            <div class="item-list" id="coursWord">
              <h2> Titre de la section Word</h2>
              {% for c in cWord %}
                <a href="#">
                  <h3> {{c.nomC}} </h3>
                  <p><small>{{c.contenueC}}</small></p>
                </a>
              {% endfor %}
            </div>
            
            <div id="coursExcel">
              <h2> Titre de la section Excel</h2>
              {% for c in cExcel %}
                <a href="#">
                  <h3> {{c.nomC}} </h3>
                  <p><small>{{c.contenueC}}</small></p>
                </a>
              {% endfor %}
            </div>
              
            <div id="coursPowerPoint">
              <h2> Titre de la section PowerPoint</h2>
              {% for c in cPp %}
                <a href="#">
                  <h3> {{c.nomC}} </h3>
                  <p><small>{{c.contenueC}}</small></p>
                </a>
              {% endfor %}
            </div>
          
            <div id="coursOracle">
              <h2> Titre de la section Oracle</h2>
              {% for c in cOracle %}
                <a href="#">
                  <h3> {{c.nomC}} </h3>
                  <p><small>{{c.contenueC}}</small></p>
                </a>
              {% endfor %}
            </div>
        
          </div>

          <div class="separator"></div>
        <!-- PARTIE EXERCICE -->
        
          <div class="part" id="exercice">
            <h1 class="part-heading text-center">Exercice</h1>
            <div id="exerciceWord">
              <h2> Titre de la section Word</h2>
              {% for c in eWord %}
                <a href="#">
                  <h3> {{c.nomE}} </h3>
                  <p><small>{{c.contenueE}}</small></p>
                </a>
              {% endfor %}
            </div>
            
            <div id="exerciceExcel">
              <h2> Titre de la section Excel</h2>
              {% for c in eExcel %}
                <a href="#">
                  <h3> {{c.nomE}} </h3>
                  <p><small>{{c.contenueE}}</small></p>
                </a>
              {% endfor %}
            </div>
              
            <div id="exercicePowerPoint">
              <h2> Titre de la section PowerPoint</h2>
              {% for c in ePp %}
                <a href="#">
                  <h3> {{c.nomE}} </h3>
                  <p><small>{{c.contenueE}}</small></p>
                </a>
              {% endfor %}
            </div>
          
            <div id="exerciceOracle">
              <h2> Titre de la section Oracle</h2>
              {% for c in eOracle %}
                <a href="#">
                  <h3> {{c.nomE}} </h3>
                  <p><small>{{c.contenueE}}</small></p>
                </a>
              {% endfor %}
            </div>
          </div>
        </div>

       {% endblock %}



    </body>

<footer>
    {% block footer %}

    {% endblock %}

</footer>


